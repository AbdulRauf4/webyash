<body>
    <div class="tm-preloader">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-6">
                    <div class="tm-preloader-logo">
                        <img src="~/assets/brand/YGJ-Logo.png" alt="logo">
                    </div>
                    <span class="tm-preloader-progress"></span>
                </div>
            </div>
        </div>
        <button class="tm-button tm-button-small">Cancel Preloader</button>
    </div>
    <div id="wrapper" class="wrapper">
        <div class="tm-header tm-header-sticky">
            <div class="tm-header-toparea bg-black">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-lg-8 col-12">
                            <ul class="tm-header-info">
                                <li><a href="tel:18883456789"><i class="ion-ios-telephone"></i>1-888-345-6789</a></li>
                                <li>
                                    <a href="mailto:contact@example.com">
                                        <i class="ion-android-mail"></i>contact@yashjewelers.com
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <div class="col-lg-4 col-12">
                            <div class="tm-header-options">
                                <div class="tm-dropdown tm-header-links">
                                    <button>My Account</button>
                                    <ul>
                                        <li><a href="@Url.Action("myAccount", "Home")">My Account</a></li>
                                        <li><a href="@Url.Action("LoginRegister", "Home")">Login/Register</a></li>
                                        <li><a href="@Url.Action("Cart", "Home")">Shopping Cart</a></li>
                                        <li><a href="@Url.Action("Wishlist", "Home")">Wishlist</a></li>
                                    </ul>
                                </div>
                                <div class="tm-dropdown tm-header-currency">
                                    <button>USD</button>
                                    <ul>
                                        <li><a href="#">USD</a></li>
                                        <li><a href="#">EUR</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="tm-header-middlearea bg-white">
                <div class="container">
                    <div class="tm-mobilenav"></div>
                    <div class="row align-items-center">
                        <div class="col-lg-3 col-6 order-1 order-lg-1">
                            <a href="index.html" class="tm-header-logo">
                                <img src="~/assets/brand/YGJ-Logo.png" class="img-fluid" alt="brand-logo">
                            </a>
                        </div>
                        <div class="col-lg-6 col-12 order-3 order-lg-2">
                            <form class="tm-header-search">
                                <input type="text" placeholder="Search product...">
                                <button><i class="ion-android-search"></i></button>
                            </form>
                        </div>
                        <div class="col-lg-3 col-6 order-2 order-lg-3">
                            <ul class="tm-header-icons">
                                <li>
                                    <a href="wishlist.html">
                                        <i class="ion-android-favorite-outline"></i><span>0</span>
                                    </a>
                                </li>
                                <li><a href="cart.html"><i class="ion-bag"></i><span>0</span></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="tm-header-bottomarea bg-white">
                <div class="container">
                    <nav class="tm-header-nav">
                        <ul>
                            <li><a href="index.html">Home</a></li>
                            <li><a href="@Url.Action("About", "Home")">About</a></li>
                            <li>
                                <a href="@Url.Action("Shop", "Home")">Shop</a>
                            </li>
                            <li class="tm-header-nav-megamenu">
                                <a href="index.html">Jewellery</a>
                                <ul>
                                    <li>
                                        <a href="#">Shop By Type</a>
                                        <ul>
                                            <li><a href="">Rings</a></li>
                                            <li><a href="">Pendants</a></li>
                                            <li><a href="">Ear Rings</a></li>
                                            <li><a href="">Watches</a></li>
                                            <li><a href="">Bracelets</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="#">Gems</a>
                                        <ul>
                                            <li><a href="@Url.Action("Diamonds", "Home")">Diamonds</a></li>
                                            <li><a href="@Url.Action("Stones", "Home")">Coloured Stones</a></li>
                                            <li><a href="@Url.Action("Gold", "Home")">18k Gold</a></li>
                                            <li><a href="@Url.Action("Gold", "Home")">22k Gold</a></li>
                                            <li><a href="@Url.Action("diamondlist", "Diamond")">Diamond Rates</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="#">Others</a>
                                        <ul>
                                            <li><a href="@Url.Action("SendGift", "Home")">Send Gifts</a></li>
                                        </ul>
                                    </li>
                                </ul>
                            </li>
                            <li><a href="@Url.Action("Contact", "Home")">Contact</a></li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>

        <div>
            @RenderBody()
        </div>

        <div class="tm-footer">
            <ul id="instafeed" class="tm-instaphotos"></ul>
            <div class="tm-footer-toparea tm-padding-section">
                <div class="container">
                    <div class="widgets widgets-footer row">
                        <div class="col-lg-3 col-md-6 col-12">
                            <div class="single-widget widget-info">
                                <a class="widget-info-logo" href="index.html">
                                    <img src="~/assets/brand/YGJ-Logo.png" class="img-fluid"
                                         alt="logo">
                                </a>
                                <ul>
                                    <li><b>Address :</b>2726 Avenue Papineau Montreal, Delhi, India</li>
                                    <li><b>Phone :</b><a href="tel:">+91-000000000</a></li>
                                    <li><b>Email :</b><a href="mailto:info@example.com">info@yashgems.com</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-6 col-12">
                            <div class="single-widget widget-quicklinks">
                                <h6 class="widget-title">Web Link</h6>
                                <ul>
                                    <li><a href="about.html">About Us</a></li>
                                    <li><a href="#">Delivery Info</a></li>
                                    <li><a href="#">Privacy & Policy</a></li>
                                    <li><a href="#">Returns & Refunds</a></li>
                                    <li><a href="#">Terms & Conditions</a></li>
                                    <li><a href="@Url.Action("Contact","Home")">Contact Us</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-6 col-12">
                            <div class="single-widget widget-quicklinks">
                                <h6 class="widget-title">My Account</h6>
                                <ul>
                                    <li><a href="@Url.Action("MyAccount","Home")">My account</a></li>
                                    <li><a href="@Url.Action("Cart","Home")">Cart</a></li>
                                    <li><a href="@Url.Action("Wishlist","Home")">Wishlist</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-6 col-12">
                            <div class="single-widget widget-newsletter">
                                <h6 class="widget-title">Join Our Newsletter</h6>
                                <p>
                                    Get Business news, tip and solutions to
                                    your problems from our experts.
                                </p>
                                <form id="tm-mailchimp-form" class="widget-newsletter-form">
                                    <input id="mc-email" type="text" placeholder="Enter email address">
                                    <button id="mc-submit" type="submit" class="tm-button">
                                        Subscribe Now
                                        <b></b>
                                    </button>
                                </form>
                                <div class="tm-mailchimp-alerts">
                                    <div class="tm-mailchimp-submitting"></div>
                                    <div class="mailchimp-success"></div>
                                    <div class="tm-mailchimp-error"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="tm-footer-bottomarea">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-md-7">
                            <p class="tm-footer-copyright">
                                ©
                                2024.  <b>Yash Gems & Jewellers</b>
                            </p>
                        </div>
                        <div class="col-md-5">
                            <div class="tm-footer-payment">
                                <img data-pagespeed-lazy-src="../../../../pagespeed_static/1.JiBnMqyl6S.gif" alt="payment methods"
                                     src="~/assets/images/payment-methods.png"
                                     onload="pagespeed.lazyLoadImages.loadIfVisibleAndMaybeBeacon(this);"
                                     onerror="this.onerror=null;pagespeed.lazyLoadImages.loadIfVisibleAndMaybeBeacon(this);">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <button id="back-top-top"><i class="ion-arrow-up-c"></i></button>
    </div>
    <script type="text/javascript"
            data-pagespeed-no-defer>pagespeed.lazyLoadImages.overrideAttributeFunctions();</script>
    <script src="~/assets/js/vendors/plugins.min.js"></script>
    <script src="~/assets/js/main.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>

</body>
</html>